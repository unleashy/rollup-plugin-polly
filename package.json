{
  "name": "rollup-plugin-polly",
  "version": "0.0.0",
  "author": "Emmanuelle Fernandes",
  "homepage": "https://github.com/unleashy/rollup-plugin-polly",
  "description": "Compile-time parser generator for Rollup",
  "keywords": [
    "rollup-plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unleashy/rollup-plugin-polly.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "build": "del-cli dist && tsc && rollup -c",
    "test": "uvu -r ts-node/register tests -i fixtures",
    "test:cov": "c8 --include=src -r text -r html yarn test"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@rollup/pluginutils": "^4.1.2",
    "ast-types": "^0.14.2",
    "recast": "^0.20.5"
  },
  "peerDependencies": {
    "rollup": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-sucrase": "^4.0.2",
    "@types/node": "^17.0.21",
    "c8": "^7.11.0",
    "del-cli": "^3.0.1",
    "fast-check": "^2.22.0",
    "prettier": "^2.5.1",
    "rollup": "^2.68.0",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "uvu": "^0.5.3"
  },
  "sideEffects": false
}
